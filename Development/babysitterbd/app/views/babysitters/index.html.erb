<p id="notice"><%= notice %></p>

<h1>Listing Babysitters</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lastname</th>
      <th>Document</th>
      <th>Startdate</th>
      <th>Maxcount</th>
      <th>Resume</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @babysitters.each do |babysitter| %>
      <tr>
        <td><%= babysitter.name %></td>
        <td><%= babysitter.lastname %></td>
        <td><%= babysitter.document %></td>
        <td><%= babysitter.startdate %></td>
        <td><%= babysitter.maxcount %></td>
        <td><%= babysitter.resume %></td>
        <td><%= babysitter.phone %></td>
        <td><%= babysitter.email %></td>
        <td><%= babysitter.password %></td>
        <td><%= link_to 'Show', babysitter %></td>
        <td><%= link_to 'Edit', edit_babysitter_path(babysitter) %></td>
        <td><%= link_to 'Destroy', babysitter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Babysitter', new_babysitter_path %>
